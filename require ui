				local function o()
					if #TextLabelValue_1.Text > 0 then
						pcall(Callback, TextLabelValue_1.Text)
					end
				end

				TextLabelValue_1.FocusLost:Connect(o)

				delay(0, o)
				
				local Set = {}

				function Set:SetTitle(txt)
					TextLabel_1.Text = txt
				end

				return Set
			end
			
			function section:Label(p)
				local TextLabel = Instance.new("Frame")
				local UICorner_1 = Instance.new("UICorner")
				local UIGradient_1 = Instance.new("UIGradient")
