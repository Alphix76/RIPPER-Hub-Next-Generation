				Frame_4.Size = UDim2.new(0.800000012, 0,0.800000012, 0)

				UIPadding_4.Parent = Frame_4
				UIPadding_4.PaddingLeft = UDim.new(0,10)

				TextSearch.Name = "TextLabel"
				TextSearch.Parent = Frame_4
				TextSearch.AnchorPoint = Vector2.new(0, 0.5)
				TextSearch.BackgroundColor3 = Color3.fromRGB(255,255,255)
				TextSearch.BackgroundTransparency = 1
				TextSearch.BorderColor3 = Color3.fromRGB(0,0,0)
				TextSearch.BorderSizePixel = 0
				TextSearch.CursorPosition = -1
				TextSearch.Position = UDim2.new(0, 0,0.5, 0)
				TextSearch.Size = UDim2.new(1, 0,0, 16)
				TextSearch.Font = Enum.Font.GothamBold
				TextSearch.PlaceholderColor3 = Color3.fromRGB(178,178,178)
				TextSearch.PlaceholderText = "Search . . ."
				TextSearch.RichText = true
				TextSearch.Text = ""
				TextSearch.TextColor3 = Color3.fromRGB(255,255,255)
				TextSearch.TextSize = 10
				TextSearch.TextWrapped = true
				TextSearch.TextXAlignment = Enum.TextXAlignment.Left

				UIPadding_5.Parent = Frame_3
				UIPadding_5.PaddingBottom = UDim.new(0,5)
				UIPadding_5.PaddingLeft = UDim.new(0,3)
				UIPadding_5.PaddingRight = UDim.new(0,3)
				UIPadding_5.PaddingTop = UDim.new(0,5)
				
				TextLabelValue_1:GetPropertyChangedSignal('Text'):Connect(function()
					TextLabelValue_1.TextTransparency = 1
					tw({v = TextLabelValue_1, t = 0.8, s = Enum.EasingStyle.Linear, d = "Out", g = {TextTransparency = 0}}):Play()
				end)
				
				local itemslist = {}
				local selectedValues = {}
				local selectedItem
				
				function itemslist:Clear(a)
					local function shouldClear(v)
						if a == nil then
							return true
						elseif type(a) == "string" then
							return v.Frame:FindFirstChild("TextLabel") and v.Frame.TextLabel.Text == a
						elseif type(a) == "table" then
							for _, name in ipairs(a) do
								if v.Frame:FindFirstChild("TextLabel") and v.Frame.TextLabel.Text == name then
									return true
								end
							end
						end
						return false
					end

					if Multi then
						selectedValues = {}
						TextLabelValue_1.Text = "--"
						pcall(Callback ,selectedValues)
					end

					for _, v in ipairs(ScrollingFrame_1:GetChildren()) do
						if v:IsA("Frame") and shouldClear(v) then
							if selectedItem and v.Frame:FindFirstChild("TextLabel") and v.Frame.TextLabel.Text == selectedItem then
								selectedItem = nil
								TextLabelValue_1.Text = "--"
								pcall(Callback, TextLabelValue_1.Text)
							end
							v:Destroy()
						end
					end

					if selectedItem == a or TextLabelValue_1.Text == a then
						selectedItem = nil
						TextLabelValue_1.Text = "--"
					end

					if a == nil then
						selectedItem = nil
						TextLabelValue_1.Text = "--"
					end

					Value = nil
				end
				
				function itemslist:Add(text)
					local Item_1 = Instance.new("Frame")
					local UICorner_2 = Instance.new("UICorner")
					local Frame_2 = Instance.new("Frame")
					local UIPadding_1 = Instance.new("UIPadding")
					local TextLabel_1 = Instance.new("TextLabel")
					
					Item_1.Name = "Item"
					Item_1.Parent = ScrollingFrame_1
					Item_1.BackgroundColor3 = Color3.fromRGB(88,88,88)
					Item_1.BackgroundTransparency = 0.89
					Item_1.BorderColor3 = Color3.fromRGB(0,0,0)
					Item_1.BorderSizePixel = 0
					Item_1.Size = UDim2.new(1, 0,0, 25)
					Item_1.ClipsDescendants = true

					UICorner_2.Parent = Item_1
					UICorner_2.CornerRadius = UDim.new(0,6)

					Frame_2.Parent = Item_1
					Frame_2.AnchorPoint = Vector2.new(0, 0.5)
					Frame_2.BackgroundColor3 = Color3.fromRGB(255,255,255)
					Frame_2.BackgroundTransparency = 1
					Frame_2.BorderColor3 = Color3.fromRGB(0,0,0)
					Frame_2.BorderSizePixel = 0
					Frame_2.Position = UDim2.new(0, 0,0.5, 0)
					Frame_2.Size = UDim2.new(0.800000012, 0,0.800000012, 0)

					UIPadding_1.Parent = Frame_2
					UIPadding_1.PaddingLeft = UDim.new(0,10)

					TextLabel_1.Parent = Frame_2
					TextLabel_1.AnchorPoint = Vector2.new(0, 0.5)
					TextLabel_1.BackgroundColor3 = Color3.fromRGB(255,255,255)
					TextLabel_1.BackgroundTransparency = 1
					TextLabel_1.BorderColor3 = Color3.fromRGB(0,0,0)
					TextLabel_1.BorderSizePixel = 0
					TextLabel_1.Position = UDim2.new(0, 0,0.5, 0)
					TextLabel_1.Size = UDim2.new(1, 0,0, 16)
					TextLabel_1.Font = Enum.Font.GothamBold
					TextLabel_1.RichText = true
					TextLabel_1.Text = text
					TextLabel_1.TextColor3 = Color3.fromRGB(255,255,255)
					TextLabel_1.TextSize = 10
					TextLabel_1.TextWrapped = true
					TextLabel_1.TextXAlignment = Enum.TextXAlignment.Left
					TextLabel_1.TextTransparency = 0.5
					
					local Click = click(Item_1)

					local function unselect()
						tw({v = Item_1, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {BackgroundColor3 = Color3.fromRGB(88,88,88)}}):Play()
						tw({v = TextLabel_1, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {TextTransparency = 0.5}}):Play()
					end
					local function hasselect()
						tw({v = Item_1, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {BackgroundColor3 = Color3.fromRGB(66,66,66)}}):Play()
						tw({v = TextLabel_1, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {TextTransparency = 0}}):Play()	
					end

					Click.MouseButton1Click:Connect(function()
						if Multi then
							if selectedValues[text] then
								selectedValues[text] = nil
								unselect()
							else
								selectedValues[text] = true
								hasselect()
							end
							local selectedList = {}
							for i, v in pairs(selectedValues) do
								table.insert(selectedList, i)
							end
							if #selectedList > 0 then
								TextLabelValue_1.Text = table.concat(selectedList, ", ")
							else
								TextLabelValue_1.Text = "--"
							end
							pcall(Callback, selectedList)
						else
							for i,v in pairs(ScrollingFrame_1:GetChildren()) do
								if v:IsA("Frame") then
									tw({v = v.Frame.TextLabel, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {TextTransparency = 0.5}}):Play()
									tw({v = v, t = 0.15, s = Enum.EasingStyle.Linear, d = "Out", g = {BackgroundColor3 = Color3.fromRGB(88,88,88)}}):Play()	
								end
							end
							hasselect()
							Value = text
							TextLabelValue_1.Text = text
							pcall(Callback, TextLabelValue_1.Text)
						end
					end)

					local function isValueInTable(val, tbl)
						if type(tbl) ~= "table" then
							return false
						end

						for _, v in pairs(tbl) do
							if v == val then
								return true
							end
						end
						return false
					end

					delay(0,function()
						if Multi then
							if isValueInTable(text, Value) then
								hasselect()
								selectedValues[text] = true
								local selectedList = {}
								for i, v in pairs(selectedValues) do
									table.insert(selectedList, i)
								end
								if #selectedList > 0 then
									TextLabelValue_1.Text = table.concat(selectedList, ", ")
								else
									TextLabelValue_1.Text = "--"
								end
								pcall(Callback,selectedList)
							end
						else
							if text == Value then
								hasselect()
								Value = text
								TextLabelValue_1.Text = text
								pcall(Callback,TextLabelValue_1.Text)
							end
						end
					end)
				end
				
				for i, v in ipairs(List) do
					itemslist:Add(v, i)
				end
				
				local Click = click(Dropdown)
				local isopen = false
			
				TextSearch:GetPropertyChangedSignal('Text'):Connect(function()
					local SearchT = string.lower(TextSearch.Text)
					for _, v in pairs(ScrollingFrame_1:GetChildren()) do
						if v:IsA("Frame") then
							if SearchT ~= "" and v.Frame:FindFirstChild("TextLabel") then
								if string.find(string.lower(v.Frame.TextLabel.Text), SearchT) then
									v.Visible = true
								else
									v.Visible = false
								end
							else
								v.Visible = true
							end
						end
					end

					if isopen then
						if UIListLayout_1.AbsoluteContentSize.Y + 50 >= 170 then
							tw({v = DropdownSelect, t = .5, s = Enum.EasingStyle.Exponential, d = "Out", g = {Size = UDim2.new(0, DropdownBar_1.AbsoluteSize.X, 0, 170)}}):Play()
						else
							tw({v = DropdownSelect, t = .5, s = Enum.EasingStyle.Exponential, d = "Out", g = {Size = UDim2.new(0, DropdownBar_1.AbsoluteSize.X, 0, UIListLayout_1.AbsoluteContentSize.Y + 40)}}):Play()
						end
					end
				end)
				
				DropdownSelect.Position = UDim2.new(0, DropdownBar_1.AbsolutePosition.X, 0, DropdownBar_1.AbsolutePosition.Y + DropdownBar_1.AbsoluteSize.Y + 5)
				
				local function open()
					if isopen then return end
					DropdownSelect.Visible = true
					if UIListLayout_1.AbsoluteContentSize.Y + 50 >= 170 then
						tw({v = DropdownSelect, t = .5, s = Enum.EasingStyle.Exponential, d = "Out", g = {Size = UDim2.new(0, DropdownBar_1.AbsoluteSize.X, 0, 170)}}):Play()
					else
						tw({v = DropdownSelect, t = .5, s = Enum.EasingStyle.Exponential, d = "Out", g = {Size = UDim2.new(0, DropdownBar_1.AbsoluteSize.X, 0, UIListLayout_1.AbsoluteContentSize.Y + 40)}}):Play()
					end
					tw({v = DropdownSelect, t = 0.3, s = Enum.EasingStyle.Exponential, d = "Out", g = {Position = UDim2.new(0, DropdownBar_1.AbsolutePosition.X, 0, DropdownBar_1.AbsolutePosition.Y + DropdownBar_1.AbsoluteSize.Y + 5)}}):Play()
					tw({v = ImageLabel_1, t = 0.3, s = Enum.EasingStyle.Exponential, d = "Out", g = {Rotation = -90}}):Play()
					isopen = true
				end
				local function close()
					if not isopen then return end
					local f = tw({v = DropdownSelect, t = .5, s = Enum.EasingStyle.Exponential, d = "Out", g = {Size = UDim2.new(0, DropdownBar_1.AbsoluteSize.X, 0, 0), Position = UDim2.new(0, DropdownBar_1.AbsolutePosition.X, 0, DropdownBar_1.AbsolutePosition.Y + DropdownBar_1.AbsoluteSize.Y - 5)}})
					f:Play()
					tw({v = ImageLabel_1, t = 0.3, s = Enum.EasingStyle.Exponential, d = "Out", g = {Rotation = 0}}):Play()
					f.Completed:Connect(function()
						isopen = false
						DropdownSelect.Visible = false
					end)
				end

				U.InputBegan:Connect(function(A)
					if A.UserInputType == Enum.UserInputType.MouseButton1 or A.UserInputType == Enum.UserInputType.Touch then
						local B, C = DropdownSelect.AbsolutePosition, DropdownSelect.AbsoluteSize
						if game:GetService "Players".LocalPlayer:GetMouse().X < B.X or game:GetService "Players".LocalPlayer:GetMouse().X > B.X + C.X or game:GetService "Players".LocalPlayer:GetMouse().Y < (B.Y - 20 - 1) or game:GetService "Players".LocalPlayer:GetMouse().Y > B.Y + C.Y then
							close()
						end
					end
				end)

				changecanvas(ScrollingFrame_1, UIListLayout_1, 5)

				Click.MouseButton1Click:Connect(function()
					if isopen then
						close()
					else
						open()
					end
				end)

				function itemslist:SetTitle(txt)
					TextLabel_1.Text = txt
				end
				
				return itemslist
			end
			
			function section:Button(p)
				local Callback = p.Callback or function() end
				local Button = Instance.new("Frame")
				local UICorner_1 = Instance.new("UICorner")
				local Frame_1 = Instance.new("Frame")
				local UIPadding_1 = Instance.new("UIPadding")
				local TextLabel_1 = Instance.new("TextLabel")
				local UIGradient_1 = Instance.new("UIGradient")

				Button.Name = "Button"
				Button.Parent = Func_1
				Button.BackgroundColor3 = Color3.fromRGB(87,172,139)
				Button.BorderColor3 = Color3.fromRGB(0,0,0)
				Button.BorderSizePixel = 0
				Button.Size = UDim2.new(1, 0,0, 30)
